PORT=3000

# Database Configuration
DB_HOST=127.0.0.1
DB_PORT=5432
DB_USER=your_db_user
DB_PASS=your_db_password
DB_NAME=busbell

# Firebase Admin SDK Configuration
# Firebase Console > Project Settings > Service Accounts 에서 다운로드한 서비스 계정 키 정보
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@your-project-id.iam.gserviceaccount.com
# Private Key는 줄바꿈을 \n로 인코딩해서 넣어야 함
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"

# TAGO Open API Configuration
TAGO_SERVICE_KEY=your_tago_service_key
TAGO_BASE_URL=https://apis.data.go.kr/1613000

# JWT Configuration
JWT_ACCESS_SECRET=your-access-token-secret
JWT_REFRESH_SECRET=your-refresh-token-secret

# Redis Configuration
REDIS_URL=redis://localhost:6379
